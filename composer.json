{
    "name": "fenos/notifynder",
    "description": "Management system of internal notifications for Laravel 4.*/ 5.*",
    "keywords": ["notifications", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9.1",
        "orchestra/testbench": "2.2.*",
        "laracasts/testdummy": "1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Fenos\\Notifynder\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
